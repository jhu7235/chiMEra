{
    // "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "func-names": 0,
        "no-param-reassign" : 0,
        "semi" : 1,
        "no-unused-vars" : 1,
        "comma-dangle" : 1,
        "max-len": 0
    }
}
